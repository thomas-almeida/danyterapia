{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(png|jpg|css|js|svg|ico|webp))",
      "dest": "/$1"
    },
    {
      "src": "/agendamento",
      "dest": "/agendamento.html"
    },
    {
      "src": "/obrigado",
      "dest": "/obrigado.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}